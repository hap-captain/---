/* miniprogram/pages/side/club/club.wxss */

.nav-list {
  display: flex;
  flex-wrap: wrap;
  padding: 0px 20rpx 0px;
  justify-content: space-between;
}
.titleList{
  position: relative;
  width: 40%;
  height: 100px;
  background-color: khaki;
  float: left;
}
.list
{
    position: relative;
    width: 100%;
    margin: 0 auto;
    float: left;
    height: auto;
    border-radius: 6px;
    margin-bottom: 17px;
    background-color: white;
}
.imgType
{
  position: relative;
  width: 100%;
  height: auto;
}
.imgType image
{
  width: 100%;
  height: 134px;
  border-radius: 6px 6px 0px 0px;
}
.nav-li1 {
  padding: 0rpx;
  border-radius: 12rpx;
  width: 45%;
  margin: 0 2.5% 40rpx;
  background-image: url(https://image.weilanwl.com/color2.0/cardBg.png);
  background-size: cover;
  background-position: center;
  position: relative;
  z-index: 1;
}

.titleType
{
  position:relative;
  margin-top: 6px;
  font-size:13px;
  font-weight:600;
  margin-left: 10px;
  padding-right: 10px;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.titleType1
{
  position: relative;
  font-size: 12px;
  color: grey;
  margin-left: 10px;
  margin-bottom: 8px;
  padding-right: 10px;
  margin-top: 3px;
}


.cu-list.grid>.cu-item [class*=cuIcon] {
  position: relative;
  display: block;
  margin-top: 0rpx;
  width: 102%;
  font-size: 67rpx;
}
.cu-list.grid.no-border {
  padding: 0rpx 10rpx;
}
